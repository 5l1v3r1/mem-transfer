net: "memnet/train_val_2.prototxt"
test_iter: 74
test_interval: 100
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.001
lr_policy: "fixed"
# stepsize should also be lower, as we're closer to being done
stepsize: 10000
display: 100
max_iter: 100000
momentum: 0.9
snapshot: 1000
snapshot_prefix: "memnet/part_2_snapshots/"
#type: "Nesterov"
# uncomment the following to default to CPU mode solving
# solver_mode: CPU
